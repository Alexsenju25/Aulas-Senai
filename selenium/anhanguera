    time.sleep(2)

    elemento = navegador.find_element(By.XPATH,'//*[@id="mainScroll"]/ul/li[5]' ) #Encontrar elemento pelo ID
    time.sleep(5)
    elemento = navegador.find_element(By.XPATH, '//*[@id="vueApp"]/footer/button') #Encontrar elemento pelo ID
    time.sleep(2)
